import java.io.*;
import java.util.*;
class Main {
    public static void main (String[] args) {
     Scanner sc = new Scanner(System.in);
     int n=sc.nextInt();
     int sum=0;
     for(int i=1;i<=n;i++)
     {
      int sum1=0,N=i;
      while(N>0)
      {
       sum1+=N%10;
       N/=10;
      }
      sum+=sum1;  
     }
     System.out.println(sum);
    }
}
