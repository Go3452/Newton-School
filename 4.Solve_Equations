import java.io.*;
import java.util.*; 

class Main {
    public static void main (String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        int count=0;
        int a,b;
        for(int i=0;i<n;i++)
        {
         for(int j=0;j<m;j++)
         {
          a=i*i+j;
          b=i+j*j;
          if(a==n && b==m)
           count++;
         }
        }
        System.out.println(count);
    }
}
