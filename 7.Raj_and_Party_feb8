import java.io.*; // for handling input/output
import java.util.*; // contains Collections framework
class Main {
    public static void main (String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        int z=sc.nextInt();
        int count=0;
        for(int i=1;i<=z;i++)
        {
         if(i%n==0 && i%m==0)
          count++;
        }
        System.out.println(count);
    }
}
