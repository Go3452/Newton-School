import java.io.*; 
import java.util.*; 
class Main {
    public static void main (String[] args) {
        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int n=sc.nextInt();
        int c=0;
        for(int i=0;i<=n-2;i++)
        {
         c=a^b;
         a=b;
         b=c;
        }
        if(n==0)
         System.out.println(a);
        else if(n==1)
         System.out.println(b);
        else
         System.out.println(c);
    }
}
