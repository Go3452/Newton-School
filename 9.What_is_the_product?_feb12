import java.io.*;
import java.util.Scanner;
class Main {
    public static void main (String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int sum=1;
        while(n>0)
        {
         int r=n%10;
         if(r!=2&&2!=3&&r!=5&&r!=7)
          sum*=r;
         n/=10;
        }
        System.out.println(sum);
    }
}
